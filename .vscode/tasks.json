// See https://go.microsoft.com/fwlink/?LinkId=733558
// for the documentation about the tasks.json format
{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build"
		},
		{
			"type": "npm",
			"script": "compile",
			"dependsOn": [
				{ "type": "cargo", "command": "build" },
			],
			"problemMatcher": "$tsc",
			"path": "clients/code",
			"presentation": {
				"panel": "dedicated",
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}
